{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "comma-dangle": 0,
    "strict": 0,
    "no-unused-vars": [1, {"args": "none", "varsIgnorePattern": "React"}],
    "react/react-in-jsx-scope": 1
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ]
}