{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "comma-dangle": 0,
    "strict": 0,
    "no-unused-vars": [1, {"args": "none", "varsIgnorePattern": "React"}],
    "no-console": 1,
    "react/display-name": 0,
    "react/no-unknown-property": 0,
    "react/no-did-mount-set-state": 0,
    "react/prop-types": 0
  },
  "env": {
    "meteor": true,
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "globals": {
    "Roles": false
  },
  "plugins": [
    "react"
  ]
}